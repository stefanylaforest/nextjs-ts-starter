{
  "extends": "next/core-web-vitals",
  "plugins": ["react", "react-hooks"],
  "rules": {
    "@next/next/no-img-element": "off",
    "object-curly-spacing": ["error", "always"],
    "quotes": ["error", "single"],
    "operator-linebreak": ["error", "after"],
    "react-hooks/exhaustive-deps": "off"
  },
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  }
}
